#include <bits/stdc++.h> 
int romanToInt(string s) {
   int ans=0;
    unordered_map<char,int>mpp;
    mpp['I']=1;
    mpp['V']=5;
    mpp['X']=10;
    mpp['L']=50;
    mpp['C']=100;
    mpp['D']=500;
    mpp['M']=1000;
    int n=s.size();
    int prev=mpp[s[n-1]];
    ans+=prev;
    for(int i=n-2;i>=0;i--)
    {
        int temp=mpp[s[i]];
        if(temp>=prev)
            ans+=temp;
        else
            ans-=temp;
        prev=temp;
        
    }
    return ans;
}
